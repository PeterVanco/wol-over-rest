{
  "name": "wol-over-rest",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "debug": "nodemon ./src --inspect=0.0.0.0:9222  -r ts-node/register ./src/index.ts",
    "debug:watch": "nodemon --watch ./src --inspect=0.0.0.0:9222  -r ts-node/register ./src/index.ts",
    "start": "node -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  },
  "main": "./dist/index.js",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.1",
    "@types/node": "^6.0.50",
    "@types/node-schedule": "^1.2.3",
    "@types/ping": "^0.2.0",
    "@types/rx": "^4.1.1",
    "config": "^3.1.0",
    "console-stamp": "^0.2.7",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "express": "^4.18.2",
    "moment": "^2.24.0",
    "node-wol": "^0.1.1",
    "npm": "^6.13.6",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "ts-node": "^8.1.0",
    "typescript": "^2.3.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
